[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.scss

module.name_mapper='.*\(.s?css\)' -> 'empty/object'
module.name_mapper='^graphql.*' -> 'empty/object'

# Generated by https://gist.github.com/turadg/4c11ef9ac1af7b41276d0acc4d4bf8c8 with root src
module.name_mapper='^\(component\|graphql\)\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1/\2'
module.name_mapper='^\(component\|graphql\)$' -> '<PROJECT_ROOT>/src/\1/index'
module.name_mapper='^\(environment\|environment.js\|index.jsx\|index\|routes.jsx\|routes\)$' -> '<PROJECT_ROOT>/src/\1'

suppress_comment= \\(.\\|\n\\)*\\$FlowTODO

[version]
>=0.60.0
