{
  "name": "car-web",
  "version": "0.0.1",
  "description": "Web front end for the car project",
  "main": "index.js",
  "author": "hally9k",
  "license": "MIT",
  "lint-staged": {
    "*.+(scss|tsx|ts|js)": [
      "yarn lint",
      "git add"
    ]
  },
  "scripts": {
    "start": "yarn relay & webpack-serve ./webpack.config.js",
    "test": "jest",
    "build": "rm -rf ./dist/* && yarn relay && NODE_ENV=production webpack",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions=js --no-watchman",
    "lint": "tslint -c tslint.json './src/**/*.ts*",
    "precommit": "lint-staged",
    "prepush": "yarn test"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "farce": "^0.2.6",
    "found": "^0.3.11",
    "found-relay": "0.3.0-alpha.13",
    "graphql": "^0.13.2",
    "react": "^16.3.2",
    "react-automata": "^2.0.0",
    "react-dom": "^16.3.2",
    "react-relay": "^1.6.0",
    "relay": "^0.8.0-1",
    "rxjs": "^5.5.7"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.3",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-relay": "^1.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "get-graphql-schema": "^2.1.1",
    "html-webpack-plugin": "^3.0.4",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "koa-connect": "^2.0.1",
    "koa-router": "^7.4.0",
    "lint-staged": "^7.1.1",
    "node-sass": "^4.8.3",
    "react-hot-loader": "^4.1.1",
    "react-testing-library": "^3.1.3",
    "relay-compiler": "^1.6.0",
    "relay-compiler-language-typescript": "^0.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3",
    "webpack": "^4.0.1",
    "webpack-serve": "^0.1.5"
  }
}
